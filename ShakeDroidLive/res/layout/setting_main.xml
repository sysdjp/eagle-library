<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content" android:orientation="vertical" android:id="@+id/setting_main_root">
<TextView android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/setting_helptext" android:textSize="20dp" android:textColor="#ffffff" android:background="#000000"></TextView><TableLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/setting_filetable"><TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content" android:layout_height="wrap_content"><TextView android:id="@+id/TextView02" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/setting_checkbox_vertical" android:textSize="20dp" android:textColor="#ffffff" android:background="#000000"></TextView><ImageView android:layout_height="64dp" android:layout_width="64dp" android:id="@+id/setting_preview_v" android:src="@drawable/setting_no_file"></ImageView>
<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/setting_selectbutton_vertical" android:text="@string/setting_selectbutton"></Button>


</TableRow><TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/setting_table_horizon"><TextView android:id="@+id/TextView03" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/setting_checkbox_horizontal" android:textSize="20dp" android:textColor="#ffffff" android:background="#000000"></TextView><ImageView android:layout_height="64dp" android:layout_width="64dp" android:id="@+id/setting_preview_h" android:src="@drawable/setting_no_file"></ImageView>
<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/setting_selectbutton" android:id="@+id/setting_selectbutton_horizontal"></Button>
</TableRow></TableLayout>





</LinearLayout>
